@model IEnumerable<BrandViewModel>

<div class="brands_products"><!--brands_products-->
    <h2>Бренды</h2>
    <div class="brands-name">
        <ul class="nav nav-pills nav-stacked">
            @foreach(var Brand in Model)
            {
                <li>
                    <a href="#"> 
                        <span class="pull-right">(50)</span>@Brand.Name
                    </a>
                </li>  
            }
        </ul>
    </div>
</div><!--/brands_products-->
  